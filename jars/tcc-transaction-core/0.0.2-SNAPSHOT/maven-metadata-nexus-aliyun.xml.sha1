    <script type="text/javascript">

        //显示验证码
        var capOption = {
            callback: function (retJson) {
                if (retJson.ret == 0) {
                    $("#txtRandstr").val(retJson.randstr);
                    $("#txtTicket").val(retJson.ticket);
                    // 用户验证成功
                    $('.tempVisit').click();
                }
                else {
                    //用户关闭验证码页面，没有验
                    capRefresh();
                }

            },
            type: "embed"
        };
        capInit(document.getElementById("cap_iframe"), capOption);
        //回调函数：验证码页面关闭时回调
    </script>
</html>

